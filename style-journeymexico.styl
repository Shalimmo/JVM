.range-value
	background: naranja;
	border-radius: 3px;
	color: blanco
	display: inline-block;
	font-size: 14px;
	line-height: 20px;
	margin-left: 7px;
	padding: 2px 5px;
	position: absolute;
	right 0px
	text-align: center;
	top 7px
	width: 50px;
	
	&:after
		position: absolute;
		top: 4px;
		left: -7px;
		width: 0;
		height: 0;
		border-top: 7px solid transparent;
		border-right: 7px solid naranja;
		border-bottom: 7px solid transparent;
		content: '';

