		width: 16px;
	&::-moz-range-track
		animate: 0.2s;
		background: naranja;
		border-radius: 25px;
		border: 0px solid #000101;
		box-shadow: 0px 0px 0px #000000, 0px 0px 0px #0d0d0d;
		cursor: pointer;
		height: 2px
		width: 100%;
	&::-moz-range-thumb
		background: naranja;
		border-radius: 50%
		border: 0px solid #000000;
		box-shadow: 0px 0px 0px #000000, 0px 0px 0px #0d0d0d;
		cursor: pointer;
		height: 16px;
		width: 16px;
	&::-ms-track
		animate: 0.2s;
		background: transparent;
		border-color: transparent;
		border-width: 39px 0;
		color: transparent;
		cursor: pointer;
		height: 2px
		width: 100%;
	&::-ms-fill-lower
		background: naranja;
		border-radius: 50px;
		border: 0px solid #000101;
		box-shadow: 0px 0px 0px #000000, 0px 0px 0px #0d0d0d;
	&::-ms-fill-upper
		background: naranja;
		border-radius: 50px;
		border: 0px solid #000101;
		box-shadow: 0px 0px 0px #000000, 0px 0px 0px #0d0d0d;
	&::-ms-thumb
		background: naranja;
		border-radius: 50%
		border: 0px solid #000000;
		box-shadow: 0px 0px 0px #000000, 0px 0px 0px #0d0d0d;
		cursor: pointer;
		height: 16px
		width: 16px
	&:focus
		outline: none;
		&::-webkit-slider-runnable-track
			background: naranja;	
		&::-ms-fill-lower
			background: naranja;
		&::-ms-fill-upper
			background: naranja;

.range-slider
